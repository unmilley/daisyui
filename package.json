{
  "name": "@unmilley/daisyui",
  "version": "1.0.0",
  "description": "Generating a theme for ðŸŒ¼ daisyUI in the usual way via config",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": "Alexandr Lyadov <hello@milley.uno> (https://milley.uno/)",
  "homepage": "https://github.com/unmilley/daisyui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unmilley/daisyui.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && git push --follow-tags && npm publish --access public",
    "test": "pnpm lint:fix && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "csstype": "^3.1.3",
    "daisyui": "^4.12.23"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-unjs": "^0.4.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "unbuild": "^3.2.0",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.1",
  "keywords": [
